# $Id: makefile.in,v 5.1 1998/01/13 01:05:44 tom Exp $
# make-file for COPYRITE

THIS	= copyrite

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
B	= ../bin

ALL	= \
	$B/$(THIS) \
	$B/$(THIS).txt

H_SRCS	=\
	$(THIS).h

C_SRCS	=\
	$(THIS).c\
	cleanup.c\
	format.c\
	hasident.c\
	insertat.c\
	maskit.c\
	parse.c\
	readit.c\
	removeit.c\
	superced.c

OBJS	=\
	$(THIS).o\
	cleanup.o\
	format.o\
	hasident.o\
	insertat.o\
	maskit.o\
	parse.o\
	readit.o\
	removeit.o\
	superced.o

SOURCES	= makefile.in $(C_SRCS) $(H_SRCS) $(THIS).txt

####### (Standard Productions) #################################################
all\
run_test:	$B $(SOURCES) $(ALL)
clean:				; -$(RM) $(CLEAN)
distclean:	clean		; -$(RM) $(ALL)
sources:	$(SOURCES)

lint.out:	$(C_SRCS)	; $(DATE); $(LINT) $(LINTOPTS) $? >>$@

####### (Details of Productions) ###############################################
$B:				; mkdir $@

$B/$(THIS):	$(OBJS) $(LIB_DEPS)
	-$(RM) -f $@
	$(LINK) -o $@ $(OBJS) $(LIB_ARGS)

$B/$(THIS).txt:	$(THIS).txt
	-$(RM) -f $@
	cp $? $@

$(OBJS):	$(THIS).h $(PORTUNIX_H)
