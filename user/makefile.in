# $Id: makefile.in,v 5.1 1998/01/13 01:17:13 tom Exp $

THIS	= copyrite

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Development) ##########################################################
INSTALL_MAN=$(TOP)/install_man

USE	= man
SECTION	= 1
PREFIX	= $(INSTALL_MAN)/$(USE)$(SECTION)/

####### (Standard Lists) #######################################################
IT	=\
	$(PREFIX)$(THIS).$(SECTION)
SOURCES	=\
	$(THIS).man

####### (Standard Productions) #################################################
all:		$(SOURCES)	; @echo '** use "make install"'
clean:				; -$(RM) $(CLEAN)
distclean:	clean		; -$(RM) $(ALL)
run_test:
sources:	$(SOURCES)
install:	$(IT)
deinstall:				; rm -f $(IT)

####### (Details of Productions) ###############################################
$(PREFIX)$(THIS).$(SECTION):	$(THIS).$(USE)	; $(PUT)

$(SOURCES):
